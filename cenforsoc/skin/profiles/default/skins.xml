<?xml version="1.0"?>

<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="cenforsoc">

 <object name="cenforsoc_skin_images"
    meta_type="Filesystem Directory View"
    directory="cenforsoc.skin:skins/cenforsoc_skin_images"/>
 <object name="cenforsoc_skin_templates"
    meta_type="Filesystem Directory View"
    directory="cenforsoc.skin:skins/cenforsoc_skin_templates"/>
 <object name="cenforsoc_skin_styles"
    meta_type="Filesystem Directory View"
    directory="cenforsoc.skin:skins/cenforsoc_skin_styles"/>

 <skin-path name="cenforsoc" based-on="Sunburst Theme">
  <layer name="cenforsoc_skin_images"
     insert-after="custom"/>
  <layer name="cenforsoc_skin_templates"
     insert-after="cenforsoc_skin_images"/>
  <layer name="cenforsoc_skin_styles"
     insert-after="cenforsoc_skin_templates"/>
 </skin-path>

</object>
